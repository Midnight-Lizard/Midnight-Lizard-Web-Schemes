{
  "name": "midnight-lizard-web-schemes",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/pavel-agarkov/Midnight-Lizard-Web-Schemes.git",
  "author": "Pavel Agarkov",
  "scripts": {
    "dev-test": "SET NODE_ENV=test&&karma start ClientApp/test/karma.conf.js",
    "ci-test": "karma start --single-run --browsers ChromeCI --reporters progress,tfs ClientApp/test/karma.conf.js",
    "docker-test": "cd ..&&docker-compose -f docker-compose.ci.test.yml up --build --abort-on-container-exit",
    "docker-build": "cd ..&&docker-compose -f docker-compose.ci.build.yml up --build --abort-on-container-exit",
    "prod-build": "webpack -p --env.prod",
    "dev-build": "webpack --verbose",
    "prod-install": "SET NODE_ENV=production&& npm install"
  },
  "dependencies": {
    "midnight-lizard-vendor": "2.4.0"
  },
  "devDependencies": {
    "midnight-lizard-testing": "1.1.0"
  }
}
